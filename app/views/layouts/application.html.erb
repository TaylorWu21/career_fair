<!DOCTYPE html>
<html>
  <head>
    <title>Career Fair Form</title>
    <%= render 'favicon' %>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all'%>
    <%= javascript_include_tag 'application'%>
  </head>


  <body>
	  <% if flash.any? %>
      <div>
  			<% flash.each do |key, value| %>
  				<div class="alert alert-<%= key %>">
  					<%= value %>
  				</div>
  			<% end %>
      </div>
		<% end %>
		
    <%= yield %>
  </body>
</html>

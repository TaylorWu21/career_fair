<!DOCTYPE html>
<html>
  <head>
    <title>CareerFair</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all'%>
    <%= javascript_include_tag 'application'%>
  </head>


  <body>
	  <% if flash.any? %>
			<% flash.each do |key, value| %>
				<div class="flash <%= key %>">
					<%= value %>
				</div>
			<% end %>
		<% end %>
		
    <%= yield %>
  </body>
</html>
